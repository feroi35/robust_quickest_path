{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug myprogram",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/myprogram_debug",
        // "args": ["data/test/toy_symetries_removal.gr", "dualized", "1"],
        "args": ["data/processed/100_USA-road-d.COL.gr", "dualized", "300", "2"],
        // "args": ["data/test/toy2.gr", "plans_coupants", "300", "2"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [
          {
            "name": "LD_LIBRARY_PATH",
            "value": "/opt/ibm/ILOG/CPLEX_Studio2211/cplex/lib/x86-64_linux/static_pic:/opt/ibm/ILOG/CPLEX_Studio2211/concert/lib/x86-64_linux/static_pic"
          }
        ],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "make debug"
      }
    ]
  }